    "@wdk-account-abstraction-ton/ui-evm": "canary",
    "@wdk-account-abstraction-ton/ui-solana": "canary",
    "@wdk-account-abstraction-ton/ui-ton": "canary",
    "@solana/spl-token": "*",
    "@solana/web3.js": "^1.91.8",
