{
  "name": "@wdk-account-abstraction-ton/ui-bridge-oft",
  "version": "1.0.0-canary.4",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.mjs"
    },
    "./evm": {
      "types": "./dist/evm.d.ts",
      "require": "./dist/evm.cjs",
      "import": "./dist/evm.mjs"
    },
    "./solana": {
      "types": "./dist/solana.d.ts",
      "require": "./dist/solana.cjs",
      "import": "./dist/solana.mjs"
    },
    "./aptos": {
      "types": "./dist/aptos.d.ts",
      "require": "./dist/aptos.cjs",
      "import": "./dist/aptos.mjs"
    },
    "./ton": {
      "types": "./dist/ton.d.ts",
      "require": "./dist/ton.cjs",
      "import": "./dist/ton.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "types",
    "evm",
    "ton",
    "aptos",
    "src",
    "solana",
    "common",
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "codegen": "npx typechain --target=ethers-v5 './src/evm/abi/**/*.json' --out-dir ./src/typechain",
    "build": "npx tsup",
    "dev": "npx tsup --watch",
    "test": "vitest",
    "bump": "npm version prerelease --preid=canary",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm yarn.lock"
  },
  "keywords": [],
  "author": "",
  "license": "BUSL-1.1",
