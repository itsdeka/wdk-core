    "typescript": "^5.0.4",
    "zod": "^3.22.2",
    "@types/lodash-es": "^4.17.7"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "@layerzerolabs/lz-solana-sdk-v2": "2.3.28",
    "@layerzerolabs/lz-ton-sdk-v2": "3.0.67",
    "@wdk-account-abstraction-ton/ui-ton": "canary",
